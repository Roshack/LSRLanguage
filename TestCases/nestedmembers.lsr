class T{ T lhs; T rhs; int val;}

fnmain void main() {
    T a;
    a.val = 5;
    a.lhs.val = 6;
    a.rhs.val = 10;
    T b;
    b.lhs = a;
    print(a.val);
    println(" = 5");
    print(a.lhs.val);
    println( " = 6");
    print(a.rhs.val);
    println(" = 10");
    print(b.lhs.val);
    println( " = 5");
    
}
